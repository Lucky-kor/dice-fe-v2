{
    "extends": ["airbnb", "prettier"],
    "plugins": ["react", "react-native"],
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "react-native/react-native": true
    },
    "rules": {
      "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx", ".tsx"] }],
      "no-console": "warn",
      "no-unused-vars": "warn",
      "react-native/no-inline-styles": "error"
    }
  }
  